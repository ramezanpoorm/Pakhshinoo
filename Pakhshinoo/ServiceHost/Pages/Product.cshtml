                            </table>*@
                        </div>

                        <div class="products-Tabs-panel entry-content wc-tab" id="tab-reviews" role="tabpanel" aria-labelledby="tab-title-reviews">
                            <div class="tab_content_heading">
                                <i class="fal fa-comment-alt-lines"></i>
