@page "{id}"
@model ServiceHost.Pages.ProductModel
@{
    ViewData["Title"] = "صفحه محصول";
}

<div class="main-site-wrap">
    <div class="mweb-site-mask"></div>
    <div class="site-wrap-outer clear">
        <div id="container">
            <div id="content" role="main">
                <div class="container">
                    <nav class="products-breadcrumb">
                        <a asp-page="~/Index">خانه</a>&nbsp;&#47;&nbsp;<a asp-page="/ProductCategory" asp-route-id="@Model.Product.CategoryId">@Model.Product.Category</a>&nbsp;&#47;&nbsp;@Model.Product.Name
                    </nav>

                    <div class="products-notices-wrapper"></div>

                    <div class="product type-product status-publish first instock product_cat-inner-car-interior has-post-thumbnail sale shipping-taxable purchasable product-type-variable">

                        <div class="primary_block row clearfix">
                            <div class="entry-img single_p_gallery gallery_type_h col-12 col-sm-12 col-md-12 col-lg-4" data-direction="horizontal">
                                <div class="inner">

                                    @if (Model.Product.HasDiscount)
                                    {<div class="product-label label-circle"><div class="circle"></div><span>@Model.Product.DiscountRate%</span></div>}
                                    <div class="images gallery-with-images">
                                        <div class="product-images">
                                            <div class="swiper-container" dir="rtl">
                                                <div class="swiper-wrapper">
                                                    @*<div class="swiper-slide img products-product-gallery__image">
                                                        <img src="~/ProductPictures/@Model.Product.Picture" class="img-responsive products-main-image" alt="@Model.Product.PictureAlt" />
                                                    </div>*@

                                                    @foreach (var picture in Model.Product.Pictures)
                                                    {
                                                        <div class="swiper-slide img products-product-gallery__image">
                                                            <img src="~/ProductPictures/@picture.Picture" class="img-responsive products-main-image" alt="@picture.PictureAlt" title="@picture.PictureTitle" />
                                                        </div>
                                                    }


                                                </div>
                                            </div>
                                        </div>

                                        <div class="product_tools_btn">
                                            <span class="popup-image" id="btn_popup_images"></span>
                                        </div>
                                    </div>

                                    <div class="thumbnails">
                                        <div class="product-thumbs">
                                            <div class="swiper-container" dir="rtl">
                                                <div class="swiper-wrapper">

                                                    @foreach (var picture in Model.Product.Pictures)
                                                    {
                                                        <div class="swiper-slide img products-product-gallery__image">
                                                            <img src="~/ProductPictures/@picture.Picture" class="img-responsive products-main-thumb" alt="@picture.PictureAlt" title="@picture.PictureTitle" />
                                                        </div>
                                                    }
                                                    
                                                </div>
                                                <div class="swiper-button-next swiper-button-v1-next"></div>
                                                <div class="swiper-button-prev swiper-button-v1-prev"></div>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>

                            <div class="summary entry-summary col-12 col-sm-12 col-md-12 col-lg-8">
                                <div class="single_product_head">
                                    <h1 class="product_title entry-title">@Model.Product.Name</h1>
                                    <div class="product_meta">
                                        <span class="sku_wrapper">شناسه محصول: <span class="sku">@Model.Product.Code</span></span>

                                        <span class="posted_in">دسته: <a asp-page="/ProductCategory" asp-route-id="@Model.Product.CategoryId">@Model.Product.Category</a></span>


                                    </div>
                                </div>
                                <div class="products-product-summary-line"></div>
                                <div class="row">
                                    <div class="col-12 col-sm-12 col-md-9 col-lg-9">
                                        @if (Model.Product.HasDiscount)
                                        {
                                            <p class="price">
                                                <del aria-hidden="true">
                                                    <span class="Price-amount amount"><bdi>@Model.Product.Price&nbsp;<span class="products-Price-currencySymbol">تومان</span></bdi></span>
                                                </del>  <i>|</i>  <span class="products-Price-amount amount"><bdi>@Model.Product.PriceWithDiscount&nbsp;<span class="products-Price-currencySymbol">تومان</span></bdi></span>
                                            </p>
                                        }
                                        else
                                        {
                                            <p class="price">
                                                <span class="products-Price-amount amount"><bdi>@Model.Product.Price&nbsp;<span class="products-Price-currencySymbol">تومان</span></bdi></span>
                                            </p>
                                        }

                                    <div clsss="variations_form cart">
                                        <div class="single_variation_wrap">
                                            <div class="products-variation single_variation"></div>
                                            <div class="products-variation-add-to-cart variations_button">
                                                <div class="quantity">
                                                    <label class="screen-reader-text">@Model.Product.Name</label>
                                                    <input type="number" class="input-text qty text" step="1" min="1" max="" name="quantity" value="1" title="تعداد" size="4" inputmode="numeric" id="productCount" />
                                                    <div class="plus-minus">
                                                        <div class="increase elm_qty fal fa-plus"></div>
                                                        <div class="reduced elm_qty fal fa-minus"></div>
                                                    </div>
                                                </div>

                                                <button type="button" class="single_add_to_cart_button button alt" onclick="addToCart('@Model.Product.Id', '@Model.Product.Name', '@Model.Product.DoublePrice', '@Model.Product.Picture')">افزودن به سبد خرید</button>
                                            </div>
                                        </div>
                                    </div>

                                        

                                        <span class="lead_time el_ready"><i class="fal fa-truck-loading"></i> آماده ارسال</span>

                                        <div class="clear"></div>
                                        <div class="add_to_wishlist_wrap">
                                            <a href="#" class="compare">مقایسه</a>
                                        </div>
                                        <div class="add_to_wishlist_wrap">
                                            <a href="#" class="add_to_wishlist single_add_to_wishlist">افزودن به علاقه مندی ها</a>
                                        </div>


                                    </div>

                                    <div class="single_info_box col-12 col-sm-12 col-md-3 col-lg-3">
                                        <div class="info_box">
                                            @if (Model.Product.IsStock)
                                            {
                                                <i class="fal fa-check-circle"></i><span>موجود در انبار</span>
                                            }
                                            else
                                            {
                                                <i class="fal fa-ban"></i><span>ناموجود</span>
                                            }


                                        </div>
                                        <div class="info_box hide_tablet hide_mobile"><i class="fal fa-truck"></i><span>تحویل اکسپرس</span></div>
                                        <div class="info_box hide_tablet hide_mobile"><i class="fal fa-calendar-check"></i><span>ضمانت بازگشت</span></div>
                                        <div class="info_box hide_tablet hide_mobile"><i class="fal fa-credit-card"></i><span>پرداخت در محل</span></div>
                                        <div class="info_box hide_tablet hide_mobile"><i class="fal fa-certificate"></i><span>ضمانت اصل بودن</span></div>
                                        <div class="info_box hide_tablet hide_mobile"><i class="fal fa-map"></i><span>ارسال به تمام نقاط</span></div>
                                        <div class="info_box hide_tablet hide_mobile"><i class="fal fa-gift"></i><span>بسته بندی زیبا</span></div>

                                    </div>
                                </div>
                            </div>

                        </div>


                    </div>


                    <div class="products-tabs wc-tabs-wrapper">
                        <ul class="tabs wc-tabs scrolling-wrapper" role="tablist">
